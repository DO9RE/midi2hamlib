weekdays=(Monday Tuesday Wednesday Thursday Friday Saturday Sunday)
if [[ $1 ]]; then #Triggered with parameter
# echo "test_midi_mapping called with $# parameters: $@"
  if [[ $1 =~ ^note ]]; then
    echo "$1"
  elif validate_midi_value "$@" weekdays test_result "test_midi_mapping: Boundary reached." ; then
    echo "test_midi_mapping: ${weekdays[$test_result]}"
  fi
else #Called from keyboard
  echo "test_midi_mapping was called with no or empty parameters. Normally only done from within menus."
fi
